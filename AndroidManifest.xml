<?xml version='1.0'?>
<manifest xmlns:a='http://schemas.android.com/apk/res/android' 
    package='com.pinky.tuts' 
    a:versionCode='0' 
    a:versionName='0'>
    <uses-permission a:name="android.permission.CAMERA"/>
    <uses-permission a:name="android.permission.SYSTEM_ALERT_WINDOW"/>
    <uses-feature a:name="android.hardware.camera" />
    <uses-feature a:name="android.hardware.camera.autofocus" />
    <application a:label='Pinky Background Photo Capture'>
        <activity a:name='com.pinky.tuts.Main'>
            <intent-filter>
                <category a:name='android.intent.category.LAUNCHER' />
                <action a:name='android.intent.action.MAIN' />
            </intent-filter>
        </activity>

        <service a:name='com.pinky.tuts.camera_service' />
    </application>
</manifest>